  "version": "0.2.0",
  "configurations": [
      {
          "type": "node",
          "name": "Debug Docker",
          "request": "attach",
          "protocol": "inspector",
          "address": "127.0.0.1",
          "port": 9229,
          "restart": true,
          "remoteRoot": "/app",
          "localRoot": "${workspaceFolder}",
          "skipFiles": [
            "<node_internals>/**/*.js",
            "${workspaceFolder}/node_modules/**/*.js",
          ],

      }
  ]
}
